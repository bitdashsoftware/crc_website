---
import type { CommunityEntry } from "@/types";
import HeroLayout from "@components/common/HeroLayout.astro";
import { getEntry } from "astro:content";
import frontPage from "@assets/why_women_only.jpeg";

const entry = (await getEntry("community", "why-women-only")) as CommunityEntry;
---

<HeroLayout 
  entry={entry} 
  heroImage={frontPage}
  contentClass="content-beige"
/> 