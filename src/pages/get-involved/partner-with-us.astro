---
import type { GetInvolvedEntry } from "@/types";
import HeroLayout from "@components/common/HeroLayout.astro";
import { getEntry } from "astro:content";
import partnerWithUsHero from "@assets/hero/partner_with_us.jpg";

const entry = (await getEntry("getInvolved", "partner-with-us")) as GetInvolvedEntry;
---

<HeroLayout 
  entry={entry} 
  heroImage={partnerWithUsHero}
  contentClass="content-beige"
  heroTextClass="content-lightgreen"
/> 