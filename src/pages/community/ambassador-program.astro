---
import type { CommunityEntry } from "@/types";
import HeroLayout from "@components/common/HeroLayout.astro";
import { getEntry } from "astro:content";
import ambassadorHero from "@assets/hero/ambassador_hero.jpg";

const entry = (await getEntry("community", "ambassador-program")) as CommunityEntry;
---

<HeroLayout 
  entry={entry} 
  heroImage={ambassadorHero}
  contentClass="content-beige"
  heroTextClass="content-lightgreen"
/> 